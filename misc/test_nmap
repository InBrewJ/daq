#!/bin/sh -e
#

nmap --open -oG nmap.log $TARGET_HOST

echo
echo Checking for restricted ports...

FAILED=no
for proto in telnet; do
    result=$(fgrep $proto nmap.log || true)
    if [ -n "$result" ]; then
        echo "$result"
        FAILED=yes
    fi
done

if [ $FAILED != no ]; then
    false
fi

echo No restricted ports found.
