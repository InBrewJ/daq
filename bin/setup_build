#!/bin/bash -e

echo -n "DAQ commit "
git log -n 1 --pretty=format:"%h - %an, %ar : %s" || true

echo -n "Last FAUCET commit "
(cd faucet; git log -n 1 --pretty=format:"%h - %an, %ar : %s" || true)

if [ "$MODE" == "in" ]; then
    cmd/inbuild
else
    cmd/build
fi

# Show the results.
docker images

# Clean this out b/c it'll have lots of junk in it.
rm -rf build
